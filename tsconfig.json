{
    "compilerOptions": {
        "target": "ES2020",
        "outDir": "dist",
        "lib": ["ES2020"],
        "moduleResolution": "node",
        "module": "CommonJS",
        "resolveJsonModule": true
    },
    "include": [
        "src/index.ts",
        "src/commands/**/*",
        "src/events/**/*",
        "src/types/**/*"
    ],
    "exclude": [
        "node_modules/**/*",
        "dist/**/*"
    ]
}